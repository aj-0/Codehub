#include<stdio.h>
int main()
{ int a[5]={3,5,7,9,11},key=7,i,found=0;
  for(i=0;i<5;i++)
  { if(a[i]==key)
    { found=1;
      break;
    }
  }
  if(found)
    printf("Found");
  else
    printf("Not Found");
  return 0;
}


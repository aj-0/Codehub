1.swapp without variable 
#include<stdio.h>
int main(){
   int a=1,b=7;
  a=a^b; // assigned valu to binary then xor bit change will swap 
  b=a^b;
  a=a^b;
  printf("%d %d",a,b);
    return 0;}

2.#include<stdio.h>
int main()
{
    int a =6,b=48,c=993830;
    if (a >b && a>c)
    {
       printf("%d",a)
    }
    else if (b>c )
    {
      printf("%d",b);
      
     }
      else {
            printf( "%d",c  );
           }
                     
     return 0;
   }  
                               

3.factorial 
#include<stdio.h>
int main()
{ int i,n=5,f=1;
for (i=1;i<n+1 ;i++)
{f=f*i}
printf("%d",f);
return 0;}  


4. Fibonacci 
#include<stdio.h>
int main()
{ int n=5,i,f=0;
   for (i=1;i<n;i++)
    {
      f=f+i; // new variable with loop variable for adding sequnce  nr
       }
printf("%d",f);
return 0;}

#include<stdio.h>
int main()
{ int n=5,i=0,f=0;
   while (i<n){
       f=f+i;
       i++;}
printf("%d",f);
return 0;
}

5.Reverse 
#include<stdio.h>
int main()
{ int a ,m,r ,rev,rem,i;
scanf ("%d",&a);
  for (i=a;a!=0;i++){
      r=a%10;
      rev= rev*10+r;
      a=a/10;}      
printf("%d",rev);
return 0;
}

#include<stdio.h>
int main()
{ int a ,m,r ,rev,rem;
scanf ("%d",&a);
  while (a!=0){
      r=a%10;
      rev= rev*10+r;
      a=a/10;}      
printf("%d",rev);
return 0;
}


6.
#include<stdio.h>
int main()
{ int a=15;
  if(a%3==0 && a%5==0)
    printf("FizzBuzz");
  else if(a%3==0)
    printf("Fizz");
  else if(a%5==0)
    printf("Buzz");
  else
    printf("%d",a);
  return 0;
}

7.
#include<stdio.h>
int main()
{ int i=1,sum=0;
  while(i<=10)
  { sum=sum+i;
    i++;
  }
  printf("%d",sum);
  return 0;
}

8.
#include<stdio.h>
int main()
{ int n=1234,prod=1,r;
  while(n!=0)
  { r=n%10;
    prod=prod*r;
    n=n/10;
  }
  printf("%d",prod);
  return 0;
}

9.
#include<stdio.h>
int main()
{ int i;
  for(i=1;i<=5;i++)
  { int j;
    for(j=1;j<=i;j++)
      printf("*");
    printf("\n");
  }
  return 0;
}


40
#include<stdio.h>
int main()
{ int a[3]={1,2,3},*p,i;
  p=a;
  for(i=0;i<3;i++)
    printf("%d ",*(p+i));
  return 0;
}

41
#include<stdio.h>
struct student {
  int id;
  char name[20];
};
int main()
{ struct student s = {101,"Ajay"};
  printf("%d %s",s.id,s.name);
  return 0;
}


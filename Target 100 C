44
#include<stdio.h>
int main()
{ int a[2][3]={{1,2,3},{4,5,6}},i,j;
  for(i=0;i<2;i++)
  { for(j=0;j<3;j++)
      printf("%d ",a[i][j]);
  }
  return 0;
}

45
#include<stdio.h>
int main()
{ char s[20];
  scanf("%s",s);
  int i=0;
  while(s[i]!='\0')
  { i++; }
  printf("%d",i);
  return 0;
}


24.
#include<stdio.h>
int main()
{ int i=2,n=10;
  while(i<=n)
  { printf("%d ",i);
    i+=2;
  }
  return 0;
}

25.
#include<stdio.h>
int main()
{ int n=153,temp=n,sum=0,r;
  while(n!=0)
  { r=n%10;
    sum=sum+r*r*r;
    n=n/10;
  }
  if(sum==temp)
    printf("Armstrong");
  else
    printf("Not Armstrong");
  return 0;
}

26.
#include<stdio.h>
int main()
{ int a,b;
  a=10;
  b=20;
  a=a+b;
  b=a-b;
  a=a-b;
  printf("%d %d",a,b);
  return 0;
}

27.
#include<stdio.h>
int main()
{ int a[5]={1,4,3,5,2},i,j,temp;
  for(i=0;i<5;i++)
  { for(j=i+1;j<5;j++)
    { if(a[i]>a[j])
      { temp=a[i];
        a[i]=a[j];
        a[j]=temp;
      }
    }
  }
  for(i=0;i<5;i++)
    printf("%d ",a[i]);
  return 0;
}

28.
#include<stdio.h>
int main()
{ int a[5]={1,4,2,5,3},i,key=5,found=0;
  for(i=0;i<5;i++)
  { if(a[i]==key)
    { found=1;
      break;
    }
  }
  if(found)
    printf("Found");
  else
    printf("Not Found");
  return 0;
}

